    cf:canonical_units "s" ;
    cf:description "The WMO definition of sunshine is that the surface incident radiative flux from the solar beam (i.e. excluding diffuse skylight) exceeds 120 W m-2. \"Duration\" is the length of time for which a condition holds." ;
    cf:grib "" ;
    cf:name "duration_of_sunshine" .

cf:dynamic_tropopause_potential_temperature
